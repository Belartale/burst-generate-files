{
    "name": "burst-generate-files",
    "version": "0.12.19",
    "description": "package module for generate files",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "Apache-2.0",
    "author": "Artur Bielan (belartale@gmail.com), Oleksandr Konik (arziburst@gmail.com)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Belartale/burst-generate-files.git"
    },
    "scripts": {
        "build": "cross-env-shell NODE_ENV=production ts-node -P \"./tsconfig.webpack.json\" \"./scripts/webpack/build\"",
        "start": "cross-env-shell TEST_MODE=development npm run start:gen",
        "prestart:build": "npm run build",
        "start:build": "cross-env-shell TEST_MODE=production npm run start:gen",
        "start:gen": "cross-env-shell ts-node -P \"./test/tsconfig.generate.json\" \"./test/generate\"",
        "prelink": "npm run build",
        "link": "npm link",
        "prepublish": "npm run build",
        "publish": "npm publish",
        "prepublish:beta": "npm run build",
        "publish:beta": "npm publish --tag beta",
        "clean": "rm -rf node_modules"
    },
    "keywords": [
        "generate",
        "generation",
        "typescript",
        "node",
        "generate-files",
        "generation-files",
        "custom-files",
        "generate-code",
        "generation-code",
        "template",
        "generate-template",
        "generation-template",
        "generator",
        "generator-files"
    ],
    "devDependencies": {
        "@types/app-root-path": "^1.2.4",
        "@types/node": "^17.0.41",
        "@types/terser-webpack-plugin": "^5.2.0",
        "@types/webpack-merge": "^5.0.0",
        "@types/webpack-node-externals": "^2.5.3",
        "@types/webpackbar": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "app-root-path": "^3.1.0",
        "babel-eslint": "^10.1.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "detect-port-alt": "1.1.6",
        "dotenv": "^16.0.1",
        "eslint": "^8.17.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.26.0",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.3",
        "webpack": "^5.73.0",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0",
        "webpackbar": "^5.0.2"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "enquirer": "^2.3.6"
    }
}